.mixhell
    &__light-text,
    &__inner-rect
        fill: #FFF

    &__spaces &
        &__lettering
            fill:#FFFFFF
        &__lines
            display: none
        &__outer-square
            fill: #F01C2E


    &__highly-explicit &
        &__lettering
            fill:#FFFFFF
        &__lines
            fill:#FF0000
        &__square
            display: none

